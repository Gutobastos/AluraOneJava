alert('Bem vindo ao jogo descubra o número secreto !');
let numeroSecreto = 5;
console.log(numeroSecreto);
let chute = prompt('Escolha um número entre 1 - 10');

if(numeroSecreto == chute) {
    alert (`Isso aí, você acertou o número sercreto: ${numeroSecreto}`)
} else {
    alert ('Você erro o número!')
}
